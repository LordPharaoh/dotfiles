execute pathogen#infect()
set clipboard=unnamedplus
set cursorline
set cursorcolumn
syntax enable
set background=dark
colorscheme solarized
set mouse=""
set autoindent
set rnu
set nu
set paste
imap <tab><tab> <c-n><CR>
nmap <S-Enter> O<Space><Esc>
nmap <CR> o<Space><Esc>
nnoremap <Space>b ?
nnoremap <Space> /
nnoremap k gk
nnoremap ; :
nnoremap j gj
cnoremap <Esc> <Return>
set tabstop=4 
set shiftwidth=4 
set foldmethod=indent 
nmap <F8> :TagbarToggle<CR>
set autoindent
